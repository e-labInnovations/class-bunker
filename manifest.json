{
    "manifest_version": 3,
    "name": "Class Bunker",
    "version": "1.0.0",
    "description": "Etlab class bunker for B.tech students",
    "permissions": [],
    "content_scripts": [{
       "matches": ["https://*.etlab.in/*"],
       "js": ["script.js"],
       "css": ["style.css"]
    }],
    "action": {
       "default_title": "Class Bunker",
       "default_popup": "popup.html",
       "default_icon": {
          "16": "assets/icons/icon-16x16.png",
          "19": "assets/icons/icon-19x19.png",
          "38": "assets/icons/icon-38x38.png",
          "48": "assets/icons/icon-48x48.png",
          "128": "assets/icons/icon-128x128.png"
       }
    },
    "icons": {
      "16": "assets/icons/icon-16x16.png",
      "19": "assets/icons/icon-19x19.png",
      "38": "assets/icons/icon-38x38.png",
      "48": "assets/icons/icon-48x48.png",
      "128": "assets/icons/icon-128x128.png"
   }
 }
 